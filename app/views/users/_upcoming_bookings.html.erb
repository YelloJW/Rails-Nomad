 <div class="upcoming-bookings">
  <h4>Upcoming bookings</h4>
  <% upcoming_bookings = current_user.bookings.select { |booking| booking.from_date > Date.today } %>
  <% if upcoming_bookings != [] %>
    <% current_user.bookings.each do |booking| %>
        <div class="dashboard-list mb-4">
          <%= link_to flat_path(booking.flat) do %>
            <img src="https://images.unsplash.com/photo-1536395155544-a3ba483e0b9b?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=2550&q=80" class="card-img-booking mr-1" alt="Yurt in the countryside">
          <% end %>
          <h5><%= booking.flat.name %></h5>
          <p>From: <strong><%= booking.from_date %></strong></p>
          <p>To: <strong><%= booking.to_date %></strong></p>
        </div>
    <% end %>
  <% else %>
    <p>You don't have any upcoming bookings.</p>
  <% end %>
</div>

